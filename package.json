{
  "name": "serietracker",
  "version": "1.0.0",
  "description": "\"Digiturk TV Program Schedule Search & Notification\"",
  "main": "main.js",
  "devDependencies": {},
  "scripts": {
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=logo/tuxpi.icns --prune=true --out=release-builds",
    "package-win": "electron-packager .  --overwrite --asar=true --platform=win32 --arch=ia32 --icon=logo/ico.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"TVSeries Tracker - GoktugCetin\"",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=logo/tuxpi.icns --prune=true --out=release-builds"
  },
  "author": "goktug-cetin",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "easy-autocomplete": "^1.3.5",
    "electron": "^4.0.0",
    "electron-prebuilt": "^1.4.13",
    "electron-store": "^2.0.0"
  }
}
